<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpWrongEmailVM" type="com.mohamedslahsapplication.app.modules.signupwrongemail.data.viewmodel.SignUpWrongEmailVM"/>
    </data>
    <FrameLayout android:id="@+id/frameSignupwrong"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageBackgroundshap"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            imageRes="@{@drawable/img_backgroundshap_green_900}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_backgroundshap_green_900"/>
        <LinearLayout android:id="@+id/linearColumncontrast"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_width="match_parent" android:orientation="vertical">
            <ImageView android:id="@+id/imageContrast"
                android:layout_height="@dimen/_75pxh"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="@dimen/_88pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_contrast_2"
                tools:ignore="ContentDescription" tools:src="@drawable/img_contrast_2"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSignup"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_119pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{signUpWrongEmailVM.signUpWrongEmailModel.txtSignup}"
                style="@style/txtRubikregular28" tools:text="@string/lbl_sign_up"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtEmailAddress"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{signUpWrongEmailVM.signUpWrongEmailModel.txtEmailAddress}"
                style="@style/txtRobotoregular14_2" tools:text="@string/lbl_email_address"/>
            <EditText android:ellipsize="end"
                android:hint="@string/msg_tim_jennings_ex2"
                android:id="@+id/etGroupFortyNine"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_11pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:singleLine="true"
                android:text="@={signUpWrongEmailVM.signUpWrongEmailModel.etGroupFortyNineValue}"
                android:textColorHint="@color/white_A700"
                style="@style/etRobotoregular14" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTheemailaddre"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{signUpWrongEmailVM.signUpWrongEmailModel.txtTheemailaddre}"
                style="@style/txtRobotolight13" tools:text="@string/msg_the_email_addre"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPassword"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{signUpWrongEmailVM.signUpWrongEmailModel.txtPassword}"
                style="@style/txtRobotoregular14" tools:text="@string/lbl_password"/>
            <EditText android:drawableEnd="@drawable/img_eye"
                android:drawablePadding="@dimen/_30pxh"
                android:ellipsize="end" android:hint="@string/lbl"
                android:id="@+id/etGroupFiftyOne"
                android:inputType="textPassword"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_8pxv"
                android:paddingEnd="@dimen/_16pxh"
                android:paddingTop="@dimen/_3pxv"
                android:singleLine="true"
                android:text="@={signUpWrongEmailVM.signUpWrongEmailModel.etGroupFiftyOneValue}"
                android:textColorHint="@color/white_A700"
                style="@style/etRobotoregular14" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtForgotPassword"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{signUpWrongEmailVM.signUpWrongEmailModel.txtForgotPassword}"
                style="@style/txtRobotoregular16_1" tools:text="@string/msg_forgot_password"/>
            <LinearLayout android:id="@+id/linearSigninButton"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_79pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylecornerRadius_1">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSignupOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_25pxv"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_27pxv"
                    android:layout_weight="0.75"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{signUpWrongEmailVM.signUpWrongEmailModel.txtSignupOne}"
                    style="@style/txtRobotoregular16" tools:text="@string/lbl_sign_up"/>
                <ImageView android:id="@+id/imageArrowright"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_27pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_195pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
