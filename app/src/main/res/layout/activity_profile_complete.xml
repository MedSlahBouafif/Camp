<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profileCompleteVM" type="com.mohamedslahsapplication.app.modules.profilecomplete.data.viewmodel.ProfileCompleteVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearProfileComplet"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnvector"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStyle">
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackvector"
                android:layout_height="@dimen/_188pxv" android:layout_width="match_parent">
                <ImageView android:id="@+id/imageVector"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_188pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_vector_teal_400}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector_teal_400"/>
                <LinearLayout android:id="@+id/linearColumnarrowleft"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8pxv"
                    android:layout_marginEnd="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_30pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <FrameLayout android:id="@+id/frameCamera"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_134pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_136pxh" style="@style/groupStyleteal_400cornerRadius">
                        <ImageView android:id="@+id/imageRectangleFour"
                            android:layout_gravity="center"
                            android:layout_height="@dimen/_126pxh"
                            android:layout_marginBottom="@dimen/_6pxv"
                            android:layout_marginEnd="@dimen/_5pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_126pxh"
                            android:scaleType="fitXY"
                            cornerRadius="@{@dimen/_36pxh}"
                            imageRes="@{@drawable/img_rectangle4}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle4"/>
                    </FrameLayout>
                </LinearLayout>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtUsername"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_37pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{profileCompleteVM.profileCompleteModel.txtUsername}"
                style="@style/txtRobotoregular14" tools:text="@string/lbl_username"/>
            <EditText android:ellipsize="end"
                android:hint="@string/lbl_mattyice"
                android:id="@+id/etGroupTwentyFive"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_6pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:singleLine="true"
                android:text="@={profileCompleteVM.profileCompleteModel.etGroupTwentyFiveValue}"
                android:textColorHint="@color/white_A700"
                style="@style/etRobotoregular14" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFirstName"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{profileCompleteVM.profileCompleteModel.txtFirstName}"
                style="@style/txtRobotoregular14" tools:text="@string/lbl_first_name"/>
            <EditText android:ellipsize="end"
                android:hint="@string/lbl_shawn"
                android:id="@+id/etGroupTwentySeven"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_8pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:singleLine="true"
                android:text="@={profileCompleteVM.profileCompleteModel.etGroupTwentySevenValue}"
                android:textColorHint="@color/white_A700"
                style="@style/etRobotoregular14" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLastName"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{profileCompleteVM.profileCompleteModel.txtLastName}"
                style="@style/txtRobotoregular14" tools:text="@string/lbl_last_name"/>
            <EditText android:ellipsize="end"
                android:hint="@string/lbl_hawkins"
                android:id="@+id/etGroupTwentyNine"
                android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_8pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:singleLine="true"
                android:text="@={profileCompleteVM.profileCompleteModel.etGroupTwentyNineValue}"
                android:textColorHint="@color/white_A700"
                style="@style/etRobotoregular14" tools:ignore="Autofill"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDateofBirth"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{profileCompleteVM.profileCompleteModel.txtDateofBirth}"
                style="@style/txtRobotoregular14" tools:text="@string/lbl_date_of_birth"/>
            <EditText android:ellipsize="end"
                android:hint="@string/lbl_12_06_1983"
                android:id="@+id/etDate" android:inputType="text"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_8pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:singleLine="true"
                android:text="@={profileCompleteVM.profileCompleteModel.etDateValue}"
                android:textColorHint="@color/white_A700"
                style="@style/etRobotoregular14" tools:ignore="Autofill"/>
            <LinearLayout android:gravity="center_vertical"
                android:id="@+id/linearCompletebutton"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_94pxv"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_79pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylewhite_2">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtComplete"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_25pxv"
                    android:layout_marginStart="@dimen/_111pxh"
                    android:layout_marginTop="@dimen/_27pxv"
                    android:layout_weight="0.79"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{profileCompleteVM.profileCompleteModel.txtComplete}"
                    style="@style/txtRobotoregular16" tools:text="@string/lbl_complete"/>
                <ImageView android:id="@+id/imageCheckmark"
                    android:layout_height="@dimen/_13pxh"
                    android:layout_marginBottom="@dimen/_30pxv"
                    android:layout_marginEnd="@dimen/_111pxh"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_marginTop="@dimen/_29pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_checkmark_teal_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_checkmark_teal_900"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
